{
    "version":1,
    "builds":[
        {
            "src":"rmt/wsgi.py",
            "use":"@versel/python",
            "config":{"maxLambdaSize":"15Mb","runtime":"python3.9"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"rmt/wsgi.py"
        }
    ]

}